<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='com_h2_gwt_jme_example'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits -->
  <inherits name="com.google.gwt.resources.Resources" />
  
  <!-- Logging -->
  <inherits name="com.google.gwt.logging.Logging"/>
  
  
  <!-- Other module inherits -->
  <inherits name='com.JME' />
  <inherits name='com.jme3.Plugins' />
  
  <!-- Logging -->
  <inherits name='org.Slf4j' />
  
  <!--  <inherits name="com.allen_sauer.gwt.log.gwt-log-TRACE" />-->
  <extend-property name="log_level" values="DEBUG,INFO,TRACE" />
  <set-configuration-property name="log_pattern" value="(%c) %d [%-5p] %m%n" />
  <set-property name="log_ConsoleLogger" value="DISABLED" />
  <set-property name="log_DivLogger" value="DISABLED" />
  <set-property name="log_FirebugLogger" value="DISABLED" />
  <set-property name="log_GWTLogger" value="DISABLED" />
  <set-property name="log_SystemLogger" value="ENABLED" />
  <set-property name="log_WindowLogger" value="DISABLED" />
  

  <!-- Specify the paths for translatable code -->
  <source path="client">
  
  </source>


  <!-- Specify the app entry point class. -->
  <entry-point class='com.h2.gwt.jme.example.client.ExampleEntryPoint' />
  
  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />
  
</module>
